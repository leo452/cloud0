<h1 align="center">Lista de permisos</h1>
<div align="center">
  <%= will_paginate %>
  <% @permissions.each do |permission| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{permission.name}", permission_path(permission) %>
          </li>
          <li><small><%= pluralize(permission.rols.count, "rol") %></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <%= will_paginate %>
</div>